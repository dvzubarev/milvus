version: 0.1

server_config:
  address: 127.0.0.1
  port: 19530                       
  deploy_mode: single               
  time_zone: UTC+8              

db_config:
  primary_path: /tmp/milvus
  secondary_path:                   
  backend_url: sqlite://:@:/      
  insert_buffer_size: 2
  preload_table:        

metric_config:
  enable_monitor: false
  collector: prometheus      
  prometheus_config:
    port: 8080                   

cache_config:
  cpu_cache_capacity: 4
  cache_insert_data: false   

engine_config:
  use_blas_threshold: 20
  gpu_search_threshold: 0

gpu_resource_config:
  enable: true
  cache_capacity: 4
  search_resources:              
    - gpu0
  build_index_resources:
    - gpu1
